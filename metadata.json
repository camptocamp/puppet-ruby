{
  "name": "camptocamp-ruby",
  "version": "1.1.3",
  "author": "Camptocamp",
  "summary": "Camptocamp Ruby Module",
  "license": "Apache-2.0",
  "source": "https://github.com/camptocamp/puppet-ruby",
  "project_page": "https://github.com/camptocamp/puppet-ruby",
  "issues_url": "https://github.com/camptocamp/puppet-ruby/issues",
  "description": "Ruby Module for Puppet",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=3.2.0 <5.0.0"
    },
    {
      "name": "camptocamp/buildenv",
      "version_requirement": ">=0.0.2 <1.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    }
  ]
}
